{
    "presets": [
      "next/babel"
    ],
    "plugins": [
      ["module-resolver", {
        "root": ["./src"],
        "alias": {
          "@styles": "./src/styles",
          "@components": "./src/components"
        }
      }],
      ["styled-components"], 
      ["@babel/plugin-syntax-top-level-await"],
      ["macros"]
    ]
  }